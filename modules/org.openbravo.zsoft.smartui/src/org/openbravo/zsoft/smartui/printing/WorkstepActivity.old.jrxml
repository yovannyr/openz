zssi_getElementTextByColumname('no',$P{LANGUAGE}) as no,
--Machine
mach.name as machine,
--Activities
zssi_getusername(a.employee_id) as emp,
a.datefrom as pd,
a.shift as shift,
to_number(a.specification) as spec,
--Production
p.name as pa,
p.value as pv,
p.value||'c' as pvcnr,
--Production Workstep
ws.name as ag,
ws.value as av,
ws.qty as qty,
ws.weightproportion as weightp,
