      where ci.C_Currency_ID=c.C_Currency_ID    
	  and ci.AD_Client_ID=I_Product.AD_Client_ID) 
      WHERE C_Currency_ID IS NULL AND ISO_Code IS NULL
      AND I_IsImported<>'Y'
      AND AD_Client_ID = ?
      ]]>
