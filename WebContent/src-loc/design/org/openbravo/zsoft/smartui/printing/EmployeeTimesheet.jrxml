	<queryString>
		<![CDATA[SELECT
-- Columns of Report
 $P{DOCUMENT_ID} as id,
 (SELECT name from ad_user where ad_user.c_bpartner_id=$P{DOCUMENT_ID}) as name,
 (SELECT o.name from ad_user,ad_org o where ad_user.c_bpartner_id=$P{DOCUMENT_ID} AND ad_user.ad_org_id=o.ad_org_id) as orgname,
 zssi_getElementTextByColumname(mday,$P{LANGUAGE}) as mday,
 zssi_getElementTextByColumname('actualbalance',$P{LANGUAGE}) as actbalance,
 zssi_getElementTextByColumname('remaining',$P{LANGUAGE}) as remainingdays,
 zssi_strnumber(zssi_getworktimeaccountbalance($P{DOCUMENT_ID} , $P{MONTH}, $P{YEAR}),$P{LANGUAGE}) as balancenum,
 zssi_getElementTextByColumname(Replace(mm,' ',''),$P{LANGUAGE}) as mm,
 my,
 mnum,
 dworkplace as place,
 dfrom,
 dto,
dbreak,
 zssi_strnumber(dbreak,$P{LANGUAGE}) as dbreaks,
 dtravel,
 zssi_strnumber(dtravel,$P{LANGUAGE}) as dtravels,
 dtotal,
 zssi_strnumber(dtotal,$P{LANGUAGE}) as dtotals,
 wdate,
 dsoll,
 zssi_strnumber(dsoll,$P{LANGUAGE}) as dsolls,
 dnorm,
 zssi_strnumber(dnorm,$P{LANGUAGE}) as dnorms,
 dnight,
 zssi_strnumber(dnight,$P{LANGUAGE}) as dnights,
 dsat,
 zssi_strnumber(dsat,$P{LANGUAGE}) as dsats,
 dsun,
 zssi_strnumber(dsun,$P{LANGUAGE}) as dsuns,
 dholi,
 zssi_strnumber(dholi,$P{LANGUAGE}) as dholis,
 dover,
 zssi_strnumber(dover,$P{LANGUAGE}) as dovers,
 dspec,
 zssi_strnumber(dspec,$P{LANGUAGE}) as dspecs,
 dvacanc,
 zssi_strnumber(dvacanc,$P{LANGUAGE}) as dvacancs,
 dill,
 zssi_strnumber(dill,$P{LANGUAGE}) as dills,
--new
dspec2 as dspec_s,
 zssi_strnumber(dspec2,$P{LANGUAGE}) as dspec_ss,
1.0 as dspec_ot,
1.0 as dspec_st,
dtrigger as dtrigger,
 zssi_strnumber(dtrigger,$P{LANGUAGE}) as dtriggers,
-- Header Co
zssi_getElementTextByColumname('Name',$P{LANGUAGE}) as Nameh,
zssi_getElementTextByColumname('Signature',$P{LANGUAGE}) as signature,
zssi_getElementTextByColumname('Triggeramt',$P{LANGUAGE}) as trigger,
zssi_getElementTextByColumname('Employee',$P{LANGUAGE}) as employer,
zssi_getElementTextByColumname('Month',$P{LANGUAGE}) as Month,
zssi_getElementTextByColumname('Day',$P{LANGUAGE}) as Day,
zssi_getElementTextByColumname('Break',$P{LANGUAGE}) as Break,
zssi_getElementTextByColumname('Place',$P{LANGUAGE}) as Placeh,
zssi_getElementTextByColumname('Fromh',$P{LANGUAGE}) as From,
zssi_getElementTextByColumname('To',$P{LANGUAGE}) as To,
zssi_getElementTextByColumname('Special',$P{LANGUAGE}) as Special,
zssi_getElementTextByColumname('Travel',$P{LANGUAGE}) as Travel,
zssi_getElementTextByColumname('Sum',$P{LANGUAGE}) as Sum,
zssi_getElementTextByColumname('Total',$P{LANGUAGE}) as Total,
zssi_getElementTextByColumname('SOLL',$P{LANGUAGE}) as Soll,
zssi_getElementTextByColumname('Normal',$P{LANGUAGE}) as Normal,
zssi_getElementTextByColumname('Nighth',$P{LANGUAGE}) as Night,
zssi_getElementTextByColumname('Saturdayw',$P{LANGUAGE}) as Saturday,
zssi_getElementTextByColumname('Sundayw',$P{LANGUAGE}) as Sunday,
zssi_getElementTextByColumname('Holidayw',$P{LANGUAGE}) as Holiday,
zssi_getElementTextByColumname('Overtimew',$P{LANGUAGE}) as Over,
zssi_getElementTextByColumname('Vacanciesw',$P{LANGUAGE}) as Vacancies,
zssi_getElementTextByColumname('Illness',$P{LANGUAGE}) as Illness,
zssi_getElementTextByColumname('Page',$P{LANGUAGE}) as Page,
zssi_getElementTextByColumname('From',$P{LANGUAGE}) as From,
 zssi_getElementTextByColumname('Timesheet',$P{LANGUAGE}) as Timesheet,
zssi_calculatevacationaccountbalance($P{DOCUMENT_ID},$P{MONTH},$P{YEAR}) as remdays
FROM zssi_getdayset($P{DOCUMENT_ID},$P{MONTH},$P{YEAR},$P{LANGUAGE}), c_bpartner
where c_bpartner.c_bpartner_id in ($P{DOCUMENT_ID})]]>
