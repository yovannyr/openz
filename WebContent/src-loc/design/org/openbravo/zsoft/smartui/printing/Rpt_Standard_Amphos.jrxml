		c_bpartner.name as partner, -- Geschäftspartner-Name
		zssi_getusernamecomplete(m_inout.ad_user_id,$P{LANGUAGE}) as contact_name, -- Ansprechpartner beim Geschäftspartner
		c_location.address1 as address_cs, -- Straße und Hausnummer des Geschäftspartners
		c_location.address2 as address_2, -- Adresszusatz des Geschäftspartners
		c_location.postal, -- PLZ des Geschäftspartners
		c_location.city, -- Stadt des Geschäftspartners
		zssi_getlocationcountry(m_inout.c_bpartner_location_id,$P{LANGUAGE}) as bpcountry, -- Land des Geschäftspartners
		c_bpartner_location.phone, -- Telefonnummer des Geschäftspartners
		c_bpartner_location.fax, -- Faxnummer des Geschäftspartners
		c_bpartner.url, -- Website des Geschäftspartners
		c_bpartner_location.uidnumber, -- Steueridentifikationsnummer des Geschäftspartners
		c_location_description(ad_orginfo.c_location_id) as location, -- Beschreibung der Adresse des Geschäftspartners
		zssi_getlocationeori(m_inout.c_bpartner_location_id) as eori, -- Zollnummer des Geschäftspartners (je nach Adresse)
	-- Eigene Daten oder Daten des Geschäftspartners - abhängig von der Art des Dokumentes (Verkauf oder Einkauf)
		zssi_getlocationcity(m_inout.delivery_location_id) as delcity, -- Lieferort
		zssi_getlocationcountry(m_inout.delivery_location_id,$P{LANGUAGE}) as delcountry, -- Lierferland
		zssi_getlocationline1(m_inout.delivery_location_id) as deladdr1, -- Lieferadresszeile 1
		zssi_getlocationline2(m_inout.delivery_location_id) as deladdr2, -- Lieferadresszeile 1
		zssi_getlocationpostal(m_inout.delivery_location_id) as delpostal, -- Lieferadresse PLZ
	-- Eigene Daten
		c_bpartner.owncodeatpartnersite, -- Eigene Nummer beim Geschäftspartner
		zssi_getusernamecomplete(m_inout.salesrep_id,$P{LANGUAGE}) as ourrepresentative, -- Eigener Ansprechpartner
		zssi_getuseremail(m_inout.salesrep_id) as ourrepemail, -- Email des eigenen Ansprechpartners
		zssi_getuserphone(m_inout.salesrep_id) as ourrepphone, -- Telefonnummer des eigenen Ansprechpartners
		ad_image.binarydata as logo, -- Eigenes Logo
		zspr.addressheader, -- Kopfzeile/Überschrift (z.B. "Muster GmbH")
		zspr.address1, -- Adresszeile 1 aus den Druckeinstellungen
		zspr.address2, -- Adresszeile 2 aus den Druckeinstellungen
		zspr.address3, -- Adresszeile 3 aus den Druckeinstellungen
		zspr.address4, -- Adresszeile 4 aus den Druckeinstellungen
		zspr.address5, -- Adresszeile 5 aus den Druckeinstellungen
		zspr.address6, -- Telefonnummer aus den Druckeinstellungen
		zspr.address7, -- Faxnummer aus den Druckeinstellungen
		zspr.address8, -- E-Mail Adresse aus den Druckeinstellungen
		zspr.address9, -- Internet Adresse aus den Druckeinstellungen
		zspr.footer1, -- Fußzeile 1 aus den Druckeinstellungen
		zspr.footer2, -- Fußzeile 2 aus den Druckeinstellungen
		zspr.footer3, -- Fußzeile 3 aus den Druckeinstellungen
		zspr.footer4, -- Fußzeile 4 aus den Druckeinstellungen
		zspr.footer5, -- Fußzeile 5 aus den Druckeinstellungen
		zspr.onelineadr, -- Einzeilige Adresse aus den Druckeinstellungen
		zspr.isfootergroupstyle, -- Fußzeilen blockweise Ja/Nein
	-- Überflüssig und/oder zu überarbeiten
		m_inout.description,
		m_inout.documentno,
		'' as paymethod
from
	m_inout
		left join c_bpartner on m_inout.c_bpartner_id = c_bpartner.c_bpartner_id
		left join c_bpartner_location on m_inout.c_bpartner_location_id = c_bpartner_location.c_bpartner_location_id
		left join c_location on c_bpartner_location.c_location_id = c_location.c_location_id,
	ad_orginfo
		left join ad_image on ad_orginfo.your_company_document_image=ad_image.ad_image_id,
	zspr_printinfo zspr
where
	m_inout.ad_org_id = ad_orginfo.ad_org_id and
	zspr.ad_org_id=m_inout.ad_org_id and
	m_inout.m_inout_id in ($P{DOCUMENT_ID})]]>
