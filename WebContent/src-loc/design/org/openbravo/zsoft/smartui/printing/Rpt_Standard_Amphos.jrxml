	<queryString>
		<![CDATA[select
	'1' as abschnitt, -- Unterscheidung Auftrag, Rechnung, Lieferung
	-- Allgemeine Dokumentendaten
		c_order.c_order_id as id, -- Dokumenten-ID
		c_order.c_doctype_id as doctype_id, -- Dokumenten-Typ-ID (Bestellung, Anfrage, Verkaufsauftrag, Angebot, ...)
		c_order.documentno as documentno, -- Dokumenten-Nummer
		zssi_getdoctext(c_order.c_doctype_id,$P{LANGUAGE}) as title, -- Dokumenten-Titel
		zssi_2html(c_order.description) as description, -- Textmitteilung oben
		zssi_2html(c_order.deliverynotes) as payterm, -- Textmitteilung unten
		zssi_strdate(current_date,$P{LANGUAGE}) as curdate, -- Aktuelles Datum
		c_order.issotrx as issotrx, -- Ist Verkaufs- oder Einkaufsprozess (issotrx=issalesordertransaction)
		c_currency.iso_code as currency_code, -- Währungs-ID
		c_currency.cursymbol as symbol, -- Währungs-Symbol
	-- Spezielle Dokumentendaten (je nach Dokument und Einstellung vorhanden oder nicht)
		zssi_2html(zssi_getTextModuleFromDoc(C_ORDER.C_ORDER_ID,'ORDER','N', $P{LANGUAGE})) as textmodule1,  -- Textmodule oben Order
	        	zssi_2html(zssi_getTextModuleFromDoc(C_ORDER.C_ORDER_ID,'ORDER','Y', $P{LANGUAGE})) as textmodule2,  -- Textmodule unten Order
		zssi_getprojectorcostcentername(c_order.c_project_id,'') as costunit, -- Verknüpfter Kostenstelle
		zssi_getprojectorcostcentername('',c_order.a_asset_id) as costcenter, -- Verknüpfter Kostenträger (Projekt)
		zssi_strdate(c_order.dateordered,$P{LANGUAGE}) as dateordered, -- Bestelldatum
		zssi_strdate(c_order.datepromised,$P{LANGUAGE}) as datepromised, -- Vorraussichtliches Rechnungsdatum
		zssi_getpaymentterm(c_order.c_paymentterm_id,$P{LANGUAGE}) as paymentterm, -- Zahlungsbedingungen (z.B. 30 Tage netto)
		zssi_getexportterm(c_order.c_bpartner_location_id, c_order.c_doctypetarget_id,$P{LANGUAGE}) as exportterm, -- Festgelegter Zusatz für Exporte in bestimmte Länder
		zssi_getincoterms(c_order.c_incoterms_id) as incoterms, -- Beschreibung Incoterms
		zssi_getshipper(c_order.m_shipper_id) as shipper, -- Logistik-Partner
		zssi_getdeliverymethod(c_order.deliveryviarule,$P{LANGUAGE}) as deliverymethod, -- Lieferart
		zssi_strint(c_order.weight) as weight, -- Gesamtgewicht
		zssi_getuom(c_order.weight_uom,$P{LANGUAGE}) as weight_uom, -- Gewichtseinheit
		null as trackingno, -- Trackingnummer (Rückverfolgungsnummer einer Lieferung)
		null as shippingto, -- Zielhafen (für Überseefracht)
		c_order.poreference as poreference, -- Referenz zum Dokument des Geschäftspartners je nach Dokumententyp (z.B. Bestellnummer, Auftragsnummer, Angebotsnummer ...)
	-- Daten des Geschäfspartners
		c_bpartner.value as customerno, -- Geschäftspartner-Suchschlüssel
		c_bpartner.name as partner, -- Geschäftspartner-Name
		zssi_getusernamecomplete(c_order.ad_user_id,$P{LANGUAGE}) as contact_name, -- Ansprechpartner beim Geschäftspartner
		c_location.address1 as address_cs, -- Straße und Hausnummer des Geschäftspartners
		c_location.address2 as address_2, -- Adresszusatz des Geschäftspartners
		c_location.postal, -- PLZ des Geschäftspartners
		c_location.city, -- Stadt des Geschäftspartners
		zssi_getlocationcountry(c_order.c_bpartner_location_id,$P{LANGUAGE}) as bpcountry, -- Land des Geschäftspartners
		c_bpartner_location.phone, -- Telefonnummer des Geschäftspartners
		c_bpartner_location.fax, -- Faxnummer des Geschäftspartners
		c_bpartner.url, -- Website des Geschäftspartners
		c_bpartner_location.uidnumber, -- Steueridentifikationsnummer des Geschäftspartners
		c_location_description(ad_orginfo.c_location_id) as location, -- Beschreibung der Adresse des Geschäftspartners
		zssi_getlocationeori(c_order.c_bpartner_location_id) as eori, -- Zollnummer des Geschäftspartners (je nach Adresse)
	-- Eigene Daten oder Daten des Geschäftspartners - abhängig von der Art des Dokumentes (Verkauf oder Einkauf)
		case when c_order.issotrx='Y' then zssi_getlocationcity(c_order.delivery_location_id) else zssi_getcityfromwarehouse(c_order.m_warehouse_id,'y') end as delcity, -- Lieferort
		case when c_order.issotrx='Y' then zssi_getlocationcountry(c_order.delivery_location_id,$P{LANGUAGE}) else zssi_getcountryfromwarehouse(c_order.m_warehouse_id,$P{LANGUAGE},'y') end  as delcountry, -- Lierferland
		case when c_order.issotrx='Y' then zssi_getlocationline1(c_order.delivery_location_id) else zssi_getlocline1fromwarehouse(c_order.m_warehouse_id,'y') end  as deladdr1, -- Lieferadresszeile 1
		case when c_order.issotrx='Y' then zssi_getlocationline2(c_order.delivery_location_id) else zssi_getlocline2fromwarehouse(c_order.m_warehouse_id,'y') end  as deladdr2, -- Lieferadresszeile 1
		case when c_order.issotrx='Y' then zssi_getlocationpostal(c_order.delivery_location_id) else zssi_getpostalfromwarehouse(c_order.m_warehouse_id,'y') end  as delpostal, -- Lieferadresse PLZ
	-- Eigene Daten
		c_bpartner.owncodeatpartnersite, -- Eigene Nummer beim Geschäftspartner
		zssi_getusernamecomplete(c_order.salesrep_id,$P{LANGUAGE}) as ourrepresentative, -- Eigener Ansprechpartner
		zssi_getuseremail(c_order.salesrep_id) as ourrepemail, -- Email des eigenen Ansprechpartners
		zssi_getuserphone(c_order.salesrep_id) as ourrepphone, -- Telefonnummer des eigenen Ansprechpartners
		ad_image.binarydata as logo, -- Eigenes Logo
		zspr.addressheader, -- Kopfzeile/Überschrift (z.B. "Muster GmbH")
		zspr.address1, -- Adresszeile 1 aus den Druckeinstellungen
		zspr.address2, -- Adresszeile 2 aus den Druckeinstellungen
		zspr.address3, -- Adresszeile 3 aus den Druckeinstellungen
		zspr.address4, -- Adresszeile 4 aus den Druckeinstellungen
		zspr.address5, -- Adresszeile 5 aus den Druckeinstellungen
		zspr.address6, -- Telefonnummer aus den Druckeinstellungen
		zspr.address7, -- Faxnummer aus den Druckeinstellungen
		zspr.address8, -- E-Mail Adresse aus den Druckeinstellungen
		zspr.address9, -- Internet Adresse aus den Druckeinstellungen
		zspr.footer1, -- Fußzeile 1 aus den Druckeinstellungen
		zspr.footer2, -- Fußzeile 2 aus den Druckeinstellungen
		zspr.footer3, -- Fußzeile 3 aus den Druckeinstellungen
		zspr.footer4, -- Fußzeile 4 aus den Druckeinstellungen
		zspr.footer5, -- Fußzeile 5 aus den Druckeinstellungen
		zspr.onelineadr, -- Einzeilige Adresse aus den Druckeinstellungen
		zspr.isfootergroupstyle, -- Fußzeilen blockweise Ja/Nein
	-- Überflüssig und/oder zu überarbeiten
		c_order.description,
		c_order.documentno,
		'' as paymethod
from
	c_order -- Auftragstabelle
		left join c_bpartner on c_order.c_bpartner_id = c_bpartner.c_bpartner_id -- Geschäftspartnertabelle
		left join c_bpartner_location on c_order.c_bpartner_location_id = c_bpartner_location.c_bpartner_location_id -- Geschäftspartneradressentabelle
		left join c_location on c_bpartner_location.c_location_id = c_location.c_location_id -- Adressentabelle
		left join c_paymentterm on c_order.c_paymentterm_id=c_paymentterm.c_paymentterm_id -- Zahlungsbedingungentabelle
		left join c_currency on c_order.c_currency_id = c_currency.c_currency_id, -- Währungstabelle
	ad_orginfo -- Organisationsinformationentabelle
		left join ad_image on ad_orginfo.your_company_document_image=ad_image.ad_image_id, -- Bildertabelle
	zspr_printinfo zspr -- Druckeinstellungentabelle
where
	c_order.ad_org_id = ad_orginfo.ad_org_id and
	zspr.ad_org_id=c_order.ad_org_id and
	c_order.c_order_id in ($P{DOCUMENT_ID})

union

select
	'2' as abschnitt, -- Unterscheidung Auftrag, Rechnung, Lieferung
	-- Allgemeine Dokumentendaten
		c_invoice.c_invoice_id as id, -- Dokumenten-ID
		c_invoice.c_doctype_id as doctype_id, -- Dokumenten-Typ-ID (Bestellung, Anfrage, Verkaufsauftrag, Angebot, ...)
		c_invoice.documentno as documentno, -- Dokumenten-Nummer
		case when c_invoice.ispaid='n' and c_invoice.deliveryrule='r'
			then 'proforma-' || zssi_getdoctext(c_invoice.c_doctype_id,$P{LANGUAGE})
			else zssi_getdoctext(c_invoice.c_doctype_id,$P{LANGUAGE})
		end  as title, -- Dokumenten-Titel
		zssi_2html(c_invoice.description) as description, -- Textmitteilung oben
		'' as payterm, -- Textmitteilung unten
		zssi_strdate(current_date,$P{LANGUAGE}) as curdate, -- Aktuelles Datum
		c_invoice.issotrx as issotrx, -- Ist Verkaufs- oder Einkaufsprozess (issotrx=issalesordertransaction)
		c_currency.iso_code as currency_code, -- Währungs-ID
		c_currency.cursymbol as symbol, -- Währungs-Symbol
	-- Spezielle Dokumentendaten (je nach Dokument und Einstellung vorhanden oder nicht)
  	      	zssi_2html(zssi_getTextModuleFromDoc(C_INVOICE.C_INVOICE_ID,'INVOICE','N', $P{LANGUAGE})) as textmodule1, -- Textmodul oben Invoice
   	     	zssi_2html(zssi_getTextModuleFromDoc(C_INVOICE.C_INVOICE_ID,'INVOICE','Y', $P{LANGUAGE})) as textmodule2, -- Textmodul unten Invoice
		zssi_getprojectorcostcentername(c_invoice.c_project_id,'') as costunit, -- Verknüpfter Kostenstelle
		zssi_getprojectorcostcentername('',c_invoice.a_asset_id) as costcenter, -- Verknüpfter Kostenträger (Projekt)
		zssi_strdate(c_invoice.dateinvoiced,$P{LANGUAGE}) as dateordered, -- Bestelldatum
		'' as datepromised, -- Vorraussichtliches Rechnungsdatum
		zssi_getpaymentterm(c_invoice.c_paymentterm_id,$P{LANGUAGE}) as paymentterm, -- Zahlungsbedingungen (z.B. 30 Tage netto)
		zssi_getexportterm(c_invoice.c_bpartner_location_id, c_invoice.c_doctypetarget_id,$P{LANGUAGE}) as exportterm, -- Festgelegter Zusatz für Exporte in bestimmte Länder
		null as incoterms, -- Beschreibung Incoterms
		null as shipper, -- Logistik-Partner
		null as deliverymethod, -- Lieferart
		null as weight, -- Gesamtgewicht
		null as weight_uom, -- Gewichtseinheit
		null as trackingno, -- Trackingnummer (Rückverfolgungsnummer einer Lieferung)
		null as shippingto, -- Zielhafen (für Überseefracht)
		c_invoice.poreference as poreference, -- Referenz zum Dokument des Geschäftspartners je nach Dokumententyp (z.B. Bestellnummer, Auftragsnummer, Angebotsnummer ...)
	-- Daten des Geschäfspartners
		c_bpartner.value as customerno, -- Geschäftspartner-Suchschlüssel
		c_bpartner.name as partner, -- Geschäftspartner-Name
		zssi_getusernamecomplete(c_invoice.ad_user_id,$P{LANGUAGE}) as contact_name, -- Ansprechpartner beim Geschäftspartner
		c_location.address1 as address_cs, -- Straße und Hausnummer des Geschäftspartners
		c_location.address2 as address_2, -- Adresszusatz des Geschäftspartners
		c_location.postal, -- PLZ des Geschäftspartners
		c_location.city, -- Stadt des Geschäftspartners
		zssi_getlocationcountry(c_invoice.c_bpartner_location_id,$P{LANGUAGE}) as bpcountry, -- Land des Geschäftspartners
		c_bpartner_location.phone, -- Telefonnummer des Geschäftspartners
		c_bpartner_location.fax, -- Faxnummer des Geschäftspartners
		c_bpartner.url, -- Website des Geschäftspartners
		c_bpartner_location.uidnumber, -- Steueridentifikationsnummer des Geschäftspartners
		c_location_description(ad_orginfo.c_location_id) as location, -- Beschreibung der Adresse des Geschäftspartners
		null as eori, -- Zollnummer des Geschäftspartners (je nach Adresse)
	-- Eigene Daten oder Daten des Geschäftspartners - abhängig von der Art des Dokumentes (Verkauf oder Einkauf)
		null as delcity, -- Lieferort
		null as delcountry, -- Lierferland
		null as deladdr1, -- Lieferadresszeile 1
		null as deladdr2, -- Lieferadresszeile 1
		null as delpostal, -- Lieferadresse PLZ
	-- Eigene Daten
		c_bpartner.owncodeatpartnersite, -- Eigene Nummer beim Geschäftspartner
		zssi_getusernamecomplete(c_invoice.salesrep_id,$P{LANGUAGE}) as ourrepresentative, -- Eigener Ansprechpartner
		zssi_getuseremail(c_invoice.salesrep_id) as ourrepemail, -- Email des eigenen Ansprechpartners
		zssi_getuserphone(c_invoice.salesrep_id) as ourrepphone, -- Telefonnummer des eigenen Ansprechpartners
		ad_image.binarydata as logo, -- Eigenes Logo
		zspr.addressheader, -- Kopfzeile/Überschrift (z.B. "Muster GmbH")
		zspr.address1, -- Adresszeile 1 aus den Druckeinstellungen
		zspr.address2, -- Adresszeile 2 aus den Druckeinstellungen
		zspr.address3, -- Adresszeile 3 aus den Druckeinstellungen
		zspr.address4, -- Adresszeile 4 aus den Druckeinstellungen
		zspr.address5, -- Adresszeile 5 aus den Druckeinstellungen
		zspr.address6, -- Telefonnummer aus den Druckeinstellungen
		zspr.address7, -- Faxnummer aus den Druckeinstellungen
		zspr.address8, -- E-Mail Adresse aus den Druckeinstellungen
		zspr.address9, -- Internet Adresse aus den Druckeinstellungen
		zspr.footer1, -- Fußzeile 1 aus den Druckeinstellungen
		zspr.footer2, -- Fußzeile 2 aus den Druckeinstellungen
		zspr.footer3, -- Fußzeile 3 aus den Druckeinstellungen
		zspr.footer4, -- Fußzeile 4 aus den Druckeinstellungen
		zspr.footer5, -- Fußzeile 5 aus den Druckeinstellungen
		zspr.onelineadr, -- Einzeilige Adresse aus den Druckeinstellungen
		zspr.isfootergroupstyle, -- Fußzeilen blockweise Ja/Nein
	-- Überflüssig und/oder zu überarbeiten
		c_invoice.description,
		c_invoice.documentno,
		'' as paymethod
from
	c_invoice
		left join c_bpartner on c_invoice.c_bpartner_id = c_bpartner.c_bpartner_id
		left join c_bpartner_location on c_invoice.c_bpartner_location_id = c_bpartner_location.c_bpartner_location_id
		left join c_location on c_bpartner_location.c_location_id = c_location.c_location_id
		left join c_paymentterm on c_invoice.c_paymentterm_id=c_paymentterm.c_paymentterm_id
		left join c_currency on c_invoice.c_currency_id = c_currency.c_currency_id,
	ad_orginfo
		left join ad_image on ad_orginfo.your_company_document_image=ad_image.ad_image_id,
	zspr_printinfo zspr
where
	c_invoice.ad_org_id = ad_orginfo.ad_org_id and
	zspr.ad_org_id=c_invoice.ad_org_id and
	c_invoice.c_invoice_id in ($P{DOCUMENT_ID})

union

