<REPORT>
	<template file="ReportBankEdit.html"/>
	<PARAMETER id="paramDirectory" name="directory" default=""/>
	<PARAMETER id="paramLanguage" name="paramLanguage" default=""/>
	<PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
	<PARAMETER id="paramSumAmount" name="sumAmount"/>
	<PARAMETER id="param0" name="Param0" default="0"/>
	<structure name="structure1">
		<FIELD id="fieldName">name</FIELD>
		<FIELD id="fieldBankaccount">bankaccount</FIELD>
		<FIELD id="fieldTrxamt" format="euroInform">trxamt</FIELD>
		<FIELD id="fieldPayamt" format="euroInform">payamt</FIELD>
		<FIELD id="fieldBankstatementId">bankstatementId</FIELD>
		<FIELD id="fieldStatementdate">statementdate</FIELD>
		<FIELD id="fieldBegining" format="euroInform">begining</FIELD>
		<FIELD id="fieldEnding" format="euroInform">ending</FIELD>
		<FIELD id="fieldCurrency">currency</FIELD>
		<FIELD id="fieldLine">line</FIELD>
		<FIELD id="fieldDescription">description</FIELD>
		<FUNCTION id="functionSumAux1" name="SUM" format="euroInform" attribute="value">trxamt</FUNCTION>
		<FUNCTION id="functionSumAux" name="ADD" arg1="functionSumAux1" arg2="paramSumAmount" format="euroInform"/>
		<FUNCTION id="functionSumSectionAux" name="SUM" format="euroInform" attribute="value">trxamt</FUNCTION>
		<FUNCTION id="functionSumLine" name="EQUAL" arg1="functionSumAux" format="euroInform"/>
		<FUNCTION id="functionSumEnding" name="EQUAL" arg1="functionSumAux" format="euroInform"/>
		<FUNCTION id="fieldSumBeginning" name="SUBTRACT" arg1="functionSumEnding" arg2="functionSumSectionAux" format="euroInform"/>
		<FUNCTION id="fieldTrxamt" name="LT" arg1="fieldTrxamt" arg2="param0" attribute="class" replace="xx"/>
		<SUBREPORT id="reportC_ACCOUNTNUMBER" name="reportC_ACCOUNTNUMBER" report="org/openbravo/erpCommon/reference/List">
			<ARGUMENT name="parameterListSelected" withId="paramC_BANKACCOUNT_ID"/>
		</SUBREPORT>
		<SECTION id="sectionAmount" field="bankaccount"/>
		<SECTION id="sectionDate" field="statementdate"/>"
<SECTION id="sectionDetail"/>
	</structure>
</REPORT>
